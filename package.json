{
  "name": "football-results-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,json}\"",
    "type-check": "echo \"No TypeScript in this project\"",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "test:watch": "echo \"Tests not implemented yet\"",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "firebase:emulator": "firebase emulators:start --only firestore",
    "start": "npm run dev",
    "clean": "rm -rf dist node_modules/.vite",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "check-updates": "npx npm-check-updates"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "firebase": "^11.9.1",
    "lucide-react": "^0.523.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "vite": "^6.3.5"
  },
  "keywords": [
    "react",
    "vite",
    "football",
    "soccer",
    "sports",
    "firebase",
    "firestore",
    "betting",
    "analysis",
    "simulation",
    "tailwindcss",
    "lucide-react",
    "responsive",
    "brasil"
  ]
}
